{
  "name": "vuejs",
  "version": "1.0.0",
  "description": "",
  "author": "soarflat",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.1",
    "browser-sync": "^2.18.12",
    "css-loader": "^0.28.7",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "pug": "^2.0.0-rc.2",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.3.0",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.0.0"
  },
  "scripts": {
    "server": "browser-sync start --server 'docs'",
    "build:html": "pug src/pug/views -o docs -P",
    "build:css": "node-sass --output-style compressed -o docs/css src/scss",
    "build:js": "webpack",
    "build": "npm-run-all build:*",
    "watch:html": "pug src/pug/views -o docs -P -w",
    "watch:css": "npm run build:css -- --watch",
    "watch:js": "webpack --watch",
    "watch": "npm-run-all --parallel watch:*",
    "minify": "NODE_ENV=production npm run webpack -- --optimize-minimize",
    "webpack": "webpack --colors --display-error-details --progress"
  },
  "dependencies": {
    "reset-css": "^2.2.0",
    "vue": "^2.3.4",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.0"
  }
}
